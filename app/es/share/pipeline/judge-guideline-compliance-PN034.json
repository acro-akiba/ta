{
  "description": "PN034の合致可否判定をするpipeline",
  "processors": [
    {
      "script": {
        "lang": "painless",
        "source": """
          if(ctx.name =~ /COG\d{4}|KOG\d{4}|FOG\d{4}|GO:\d{7}|\d+\.-\.-\.-|\d+\.\d+\.-\.-|\d+\.\d+\.\d+\.-|\d+\.\d+\.\d+\.(n)?\d+|PF\d{5}/){
            ctx.guideline_noncompliance_list.add("PN034");
          }
          else{
            ctx.guideline_PN034 = "1";
            ctx.guideline_compliance_list.add("PN034");
          }
        """,
        "on_failure": [
          {
            "append": {
              "field": "error_pipeline",
              "value": "judge-guideline-compliance-PN034"
            }
          }
        ]
      }
    }
  ]
}
